<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
		<PackageId>ClaLuLaNa.$(AssemblyName)</PackageId>
		<Version>1.0.1</Version>
		<Title>Abstract Http Service</Title>
		<Authors>Claudio de Oliveira</Authors>
		<Description>Abstract Http Service for clients</Description>
		<PackageTags>EF;Repository;Abstract</PackageTags>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.JsonPatch" Version="6.0.13" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
		<PackageReference Include="IdentityModel" Version="6.0.0" />
	</ItemGroup>

</Project>
